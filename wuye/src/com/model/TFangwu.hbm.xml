<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.model.TFangwu" table="t_fangwu">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="50" />
        </property>
        <property name="mianji" type="java.lang.String">
            <column name="mianji" length="50" />
        </property>
        <property name="weizhi" type="java.lang.String">
            <column name="weizhi" length="50" />
        </property>
        <property name="fangwuno" type="java.lang.String">
            <column name="fangwuno" length="50" />
        </property>
        <property name="fangxing" type="java.lang.String">
            <column name="fangxing" length="50" />
        </property>
        <property name="danjia" type="java.lang.String">
            <column name="danjia" length="50" />
        </property>
        <property name="zongjia" type="java.lang.String">
            <column name="zongjia" length="50" />
        </property>
        <property name="zhuhu" type="java.lang.String">
            <column name="zhuhu" length="50" />
        </property>
        <property name="zhuhuTel" type="java.lang.String">
            <column name="zhuhu_tel" length="50" />
        </property>
        <property name="del" type="java.lang.String">
            <column name="del" length="50" />
        </property>
    </class>
</hibernate-mapping>
