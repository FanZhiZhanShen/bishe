<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.model.TTousu" table="t_tousu">
        <id name="tousuId" type="java.lang.Integer">
            <column name="tousu_id" />
            <generator class="increment" />
        </id>
        <property name="tousuShijian" type="java.lang.String">
            <column name="tousu_shijian" length="50" />
        </property>
        <property name="tousuYuanyin" type="java.lang.String">
            <column name="tousu_yuanyin" length="5000" />
        </property>
        <property name="zhuhuId" type="java.lang.Integer">
            <column name="zhuhu_id" />
        </property>
        <property name="del" type="java.lang.String">
            <column name="del" length="50" />
        </property>
        <property name="tousuZhuangtai" type="java.lang.String">
            <column name="tousu_zhuangtai" length="50" />
        </property>
    </class>
</hibernate-mapping>
