<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.model.TZhuhu" table="t_zhuhu">
        <id name="zhuhuId" type="java.lang.Integer">
            <column name="zhuhu_id" />
            <generator class="increment" />
        </id>
        <property name="zhuhuName" type="java.lang.String">
            <column name="zhuhu_name" length="50" />
        </property>
        <property name="zhuhuIdcard" type="java.lang.String">
            <column name="zhuhu_idcard" length="50" />
        </property>
        <property name="zhuhuTel" type="java.lang.String">
            <column name="zhuhu_tel" length="50" />
        </property>
        <property name="zhuhuFangxing" type="java.lang.String">
            <column name="zhuhu_fangxing" length="50" />
        </property>
        <property name="zhuhuMianji" type="java.lang.String">
            <column name="zhuhu_mianji" length="50" />
        </property>
        <property name="zhuhuShiyongmianj" type="java.lang.String">
            <column name="zhuhu_shiyongmianj" length="50" />
        </property>
        <property name="zhuhuRuzhushijian" type="java.lang.String">
            <column name="zhuhu_ruzhushijian" length="50" />
        </property>
        <property name="zhuhuCheliangxinxi" type="java.lang.String">
            <column name="zhuhu_cheliangxinxi" length="50" />
        </property>
        <property name="userName" type="java.lang.String">
            <column name="userName" length="50" />
        </property>
        <property name="userPw" type="java.lang.String">
            <column name="userPw" length="50" />
        </property>
        <property name="del" type="java.lang.String">
            <column name="del" length="50" />
        </property>
        <property name="fangwuno" type="java.lang.String">
            <column name="fangwuno" length="50" />
        </property>
    </class>
</hibernate-mapping>
