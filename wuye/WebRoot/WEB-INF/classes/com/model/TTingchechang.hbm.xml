<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.model.TTingchechang" table="t_tingchechang">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="tingchechangname" type="java.lang.String">
            <column name="tingchechangname" length="50" />
        </property>
        <property name="cheweizhuangtai" type="java.lang.String">
            <column name="cheweizhuangtai" length="50" />
        </property>
        <property name="danjia" type="java.lang.String">
            <column name="danjia" length="50" />
        </property>
        <property name="del" type="java.lang.String">
            <column name="del" length="50" />
        </property>
    </class>
</hibernate-mapping>
